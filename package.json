{
	"name": "comp-office",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"test": "cucumber-js",
		"cypress-open": "cypress open",
		"start": "next start",
		"lint": "next lint",
		"docs-prisma": "prisma generate --generator markdown && python tools/md_shift.py wiki/dev/database.md wiki/dev/database.md 2",
		"docs-merge": "python tools/md_merge.py -o wiki/wiki.md wiki/docs.md",
		"docs-format": "prettier wiki/wiki.md --write",
		"docs-story": "zsh -f tools/story.zsh",
		"docs-md": "npm run docs-prisma && npm run docs-story && npm run docs-merge && npm run docs-format",
		"docs-pdf": "cd wiki && pandoc wiki.md -F ../tools/pandoc-svg.py -F mermaid-filter -o wiki.pdf",
		"docs-docx": "cd wiki && pandoc wiki.md -F ../tools/pandoc-svg.py -F mermaid-filter -o wiki.docx",
		"docs": "npm run docs-md && npm run docs-pdf",
		"stat": "find . -type f ! -path \"./node_modules/*\" ! -path \"./.next/*\" ! -path \"./wiki/dev/*\" ! -path \"./.git/*\" ! -path \"./public/*\" ! -path \"*.err\" ! -path \"./wiki/wiki.*\" ! -path \"*.jpg\" ! -path \"./package-lock.json\" ! -path \"*.ico\" | xargs -I xx bash -c 'echo $(cat xx | wc -l)  - xx' | cat | sort -k1,1 -n"
	},
	"dependencies": {
		"@badeball/cypress-cucumber-preprocessor": "^20.0.3",
		"@cypress/browserify-preprocessor": "^3.0.2",
		"@hookform/resolvers": "^3.3.4",
		"@prisma/client": "^5.11.0",
		"@radix-ui/react-context-menu": "^2.1.5",
		"@radix-ui/react-dialog": "^1.0.5",
		"@radix-ui/react-dropdown-menu": "^2.0.6",
		"@radix-ui/react-label": "^2.0.2",
		"@radix-ui/react-menubar": "^1.0.4",
		"@radix-ui/react-popover": "^1.0.7",
		"@radix-ui/react-slot": "^1.0.2",
		"@tanstack/react-query": "^4.36.1",
		"@trpc/client": "^10.45.2",
		"@trpc/react-query": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"cmdk": "^1.0.0",
		"lucide-react": "^0.361.0",
		"next": "14.1.3",
		"next-auth": "^5.0.0-beta.15",
		"puppeteer": "^22.8.0",
		"react": "^18",
		"react-dom": "^18",
		"react-hook-form": "^7.51.1",
		"react-hot-toast": "^2.4.1",
		"react-icons": "^5.0.1",
		"react-virtualized-auto-sizer": "^1.0.24",
		"react-window": "^1.8.10",
		"tailwind-merge": "^2.2.2",
		"tailwindcss-animate": "^1.0.7",
		"uuid": "^9.0.1",
		"zod": "^3.22.4",
		"zod-validation-error": "^3.0.3"
	},
	"devDependencies": {
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@types/react-window": "^1.8.8",
		"autoprefixer": "^10.0.1",
		"cypress": "^13.7.2",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-next": "14.1.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
		"eslint-plugin-next": "^0.0.0",
		"eslint-plugin-prettier": "^5.1.3",
		"postcss": "^8",
		"prettier": "3.2.5",
		"prisma": "^5.11.0",
		"prisma-markdown": "^1.0.9",
		"tailwindcss": "^3.3.0",
		"typescript": "^5"
	}
}
